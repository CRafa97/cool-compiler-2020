Bool.Constructor:
sw $ra, 0($sp)
addiu $sp, $sp, -4
li $a0, 0
sw $a0, ($sp)
addiu $sp, $sp, -4
lw $v0, 4($sp)
li $a0, 0
sw $a0, ($sp)
addi $sp, $sp, -4
li $a0, 0
sw $a0, ($sp)
addi $sp, $sp, -4
jr $ra
Bool.type_name:
sw $ra, 0($sp)
